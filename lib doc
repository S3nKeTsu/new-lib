local UILibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/S3nKeTsu/new-lib/main/lib"))()

local MainUI = UILibrary.Load(string Title)
local FirstPage = MainUI.AddPage(string PageTitle, bool IncludeSearchBar = true)

local FirstLabel = FirstPage.AddLabel(string Text)
local FirstButton = FirstPage.AddButton(string Text, function Callback)
local FirstToggle = FirstPage.AddToggle(string Text, bool Default, function Callback)
local FirstSlider = FirstPage.AddSlider(string Text, table Configuration, function Callback)
local FirstPicker = FirstPage.AddColourPicker(string Text, variant<string/table/color3> DefaultColour, function Callback)
local FirstDropdown = FirstPage.AddDropdown(string Text, table Options, function Callback)
